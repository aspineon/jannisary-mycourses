<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>bean</managed-bean-name>
  <managed-bean-class>entities.business.Bean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>lecturerBean</managed-bean-name>
  <managed-bean-class>entities.business.LecturerBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>courseBean</managed-bean-name>
  <managed-bean-class>entities.business.CourseBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sysUserBean</managed-bean-name>
  <managed-bean-class>entities.business.SysUserBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>syllabusBean</managed-bean-name>
  <managed-bean-class>entities.business.SyllabusBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classroomBean</managed-bean-name>
  <managed-bean-class>entities.business.ClassroomBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>typeofCourseBean</managed-bean-name>
  <managed-bean-class>entities.business.TypeofCourseBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>departmentBean</managed-bean-name>
  <managed-bean-class>entities.business.DepartmentBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>demoDragDropBean</managed-bean-name>
  <managed-bean-class>entities.business.DemoDragDropBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>manuelSchedulingUtilBean</managed-bean-name>
  <managed-bean-class>entities.util.ManuelSchedulingUtilBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>deanBean</managed-bean-name>
  <managed-bean-class>entities.business.DeanBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginBean</managed-bean-name>
  <managed-bean-class>entities.business.LoginBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>deanCourseBean</managed-bean-name>
  <managed-bean-class>entities.business.DeanCourseBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/PreProd/login.jsp</from-view-id>
  <navigation-case>
   <from-action>#{loginBean.checkLogin}</from-action>
   <from-outcome>success</from-outcome>
   <to-view-id>/PreProd/index.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{loginBean.checkLogin}</from-action>
   <from-outcome>failure</from-outcome>
   <to-view-id>/PreProd/login.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>EmailValidator</validator-id>
  <validator-class>entities.validator.EmailValidator</validator-class>
 </validator>
 <validator>
  <validator-id>EmailValidatorWithRegex</validator-id>
  <validator-class>entities.validator.EmailValidatorWithRegex</validator-class>
 </validator>
</faces-config>
